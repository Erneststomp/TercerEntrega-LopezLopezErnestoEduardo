<div style="width: 100%;">

    <div >
        <form action="/logout" method="post" >
            <label>Bienvenido: {{userData.names}}</label>
            <br>
            <label>correo:{{userData.id}}</label>
            <span id="log_user"></span>
            <button id="logout">Log Out</button>
        </form>
    </div>
    <br><br>
    <div >
        <label>Go to Cart</label>
        <button id="gotoCarts">go to cart</button>
    </div>
    <br>



    <div style="display: inline-flex; width: 95%;">
        <div style="width: 90%;">
        <h1> Available Pokemons:</h1>
            <div style="display: block;  border: 1px solid red;  padding: 5px;  margin-top: 5px; width: 100%; height: 550px;overflow-y: scroll; border-style:double;">
                {{#each characters}}
                    <div>
                        <img src="{{this.resp.object.thumbnail}}" style="height: 100px;"></img>
                        <h2>Name: {{this.resp.object.name}} Price: ${{this.resp.object.price}} pokedollars</h2>
                        <button>Add to Cart</button> <button> View </button>
                    </div>
                    <br>
                {{/each}}
            </div>
        </div>
        
        
    </div>
    
</div>
 
